package com.senac.exercicio1;

public class Circle {
	double area;
	double circ;
	double raio = 2;
	
	
	private void area() {
		this.area = 3.1416 * Math.pow(this.raio, 2);
	}

	
	private void circunferencia() {

		this.circ = 2 * 3.1416 * this.raio;
	}
	
	public String toString() {
		this.circunferencia();

		return "√Årea " + this.area;
	}
	
}
