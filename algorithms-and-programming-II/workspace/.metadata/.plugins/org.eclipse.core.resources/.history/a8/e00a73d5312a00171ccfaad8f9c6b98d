package com.senac.exercicio1;

import com.senac.SimpleJava.Console;
import java.util.Scanner;

public class Main {
	public void run() {
		Circle circle;
		circle = new Circle();
		
		 double x1 = 3;
		 double y1 = 4;
		 double r1 = 3;
		 
		 double x2 = 6;
		 double y2 = 8;
		
		 double r2 = 5;
		 
		 circle.setValue(x1, y1, r1);
		 circle.setValue(x2, y2, r2);
		 
		 circle.toString();

	}
}
