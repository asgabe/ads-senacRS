package com.senac.exercicio1;

import com.senac.SimpleJava.Console;
import java.util.Scanner;

public class Main {
	public void run() {
		Circle circle;
		circle = new Circle();
		Scanner read = new Scanner(System.in);

		double x1;
		double y1;

		double x2;
		double y2;

		double r1;
		double r2;

		Console.println("Raio 1:");
		r1 = read.nextDouble();

		Console.println("Raio 2:");
		r2 = read.nextDouble();

		Console.println("X 1:");
		x1 = read.nextDouble();

		Console.println("Y 1:");
		y1 = read.nextDouble();

		Console.println("X 2:");
		x2 = read.nextDouble();

		Console.println("Y 2:");
		y2 = read.nextDouble();

	}
}
