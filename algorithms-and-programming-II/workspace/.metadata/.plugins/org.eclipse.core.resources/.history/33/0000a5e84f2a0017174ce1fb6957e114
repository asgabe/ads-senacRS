package com.senac.exercicio2;

public class Account {
	private double balance;
	
	protected void CashOut(double value) {
		if(value < balance) {
			this.balance = value - this.balance;
			this.toString("Account [balance=" + balance + "]");
		} else {
			this.toString("Insufficient funds");
		}
	}
	
	protected void CashDeposit(double value) {
		this.balance = value + this.balance;
	}
	
	protected double getBalance() {
		return this.balance;
	}

	private String toString(String msg) {
		return msg;
	}

}
