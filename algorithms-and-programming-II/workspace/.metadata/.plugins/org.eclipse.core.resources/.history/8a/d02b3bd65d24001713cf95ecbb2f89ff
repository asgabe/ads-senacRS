package com.senac.exercicio1;

public class Circle {
	double a;
	double c;
	double r = 2;
	
	
	private void area() {
		this.a = 3.1416 * Math.pow(this.r, 2);
	}

	
	private void circunferencia() {
		this.c = 2 * 3.1416 * this.r;
	}
	
	public String toString() {
		this.circunferencia();

		return "√Årea " + this.a;
	}
	
}
