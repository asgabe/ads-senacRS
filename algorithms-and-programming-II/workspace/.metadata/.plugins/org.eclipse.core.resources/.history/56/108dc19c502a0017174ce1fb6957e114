package com.senac.exercicio2;

public interface InterfaceClient {
	double balance = 500;
	
	private void CashOut(double value) {
		if(value < balance) {
			balance = value - balance;
			this.toString("Account [balance=" + balance + "]");
		} else {
			this.toString("Insufficient funds");
		}
	}
	
	private void CashDeposit(double value) {
		balance = value + balance;
	}
	
	double getBalance() {
		return balance;
	}

}
